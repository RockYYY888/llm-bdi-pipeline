# OpenAI API Configuration
OPENAI_API_KEY=sk-proj-your-api-key-here

# Optional: Model selection
OPENAI_MODEL=gpt-4o-mini

# Optional: API timeout (seconds)
OPENAI_TIMEOUT=30

# Optional: Use LLM-based planner for Stage 3 (default: false)
# Set to "true" to use LLM planner that can handle G (Globally), X (Next), and U (Until) constraints
# Set to "false" to use classical PDDL planner (pyperplan) - cannot handle temporal constraints
USE_LLM_PLANNER=false
